{
  "name": "hologram-virtual-demo",
  "version": "1.0.0",
  "description": "Hologram 12,288 Virtual Infrastructure Demo - HoloPost",
  "main": "dist/demo.js",
  "scripts": {
    "demo": "ts-node src/demo.ts",
    "demo:transport": "ts-node src/steps/01-transport.ts",
    "demo:storage": "ts-node src/steps/02-storage.ts",
    "demo:compute": "ts-node src/steps/03-compute.ts",
    "demo:encoding": "ts-node src/steps/04-encoding.ts",
    "demo:bench": "ts-node src/steps/04-bench.ts",
    "bench:25g": "cross-env HOLOGRAM_USE_MOCK=false ts-node src/steps/04-bench.ts --duration 15 --lanes 64 --payload 4096 --batch 32 --window 100 --workers 8 --target 25",
    "bench:25g:real": "cross-env HOLOGRAM_USE_MOCK=false ts-node src/steps/04-bench.ts --duration 15 --lanes 64 --payload 4096 --batch 32 --window 100 --workers 8 --target 25",
    "bench:25g:comprehensive": "node run-real-sdk-benchmark.js --comprehensive --duration 20",
    "bench:25g:stress": "node run-real-sdk-benchmark.js --stress --duration 30",
    "bench:25g:compare": "node run-real-sdk-benchmark.js --compare",
    "bench:mock-fast": "cross-env MOCK_SPEED_FACTOR=4 ts-node src/steps/04-bench.ts --duration 5 --lanes 32 --payload 4096 --batch 16 --workers 4 --target 25",
    "bench:matrix": "ts-node src/steps/04-bench.ts --matrix",
    "demo:perf": "ts-node src/demo.ts --perf",
    "demo:throughput": "ts-node src/throughput-test.ts",
    "demo:throughput:real": "ts-node src/throughput-test.ts --real",
    "demo:real-sdk-throughput": "ts-node src/real-sdk-throughput-test.ts",
    "demo:quick-throughput": "ts-node src/quick-throughput-test.ts",
    "demo:high-performance": "ts-node src/high-performance-hologram.ts",
    "demo:parallel": "ts-node src/parallel-demo.ts",
    "demo:parallel:real": "ts-node src/parallel-demo.ts --real",
    "demo:parallel:main": "ts-node src/demo.ts --parallel",
    "optimize:incremental": "ts-node src/incremental-optimization.ts",
    "optimize:enhanced": "ts-node src/optimized-throughput-test.ts",
    "optimize:run-incremental": "node run-incremental-optimization.js",
    "optimize:run-enhanced": "node run-enhanced-throughput.js",
    "encode": "ts-node src/demo.ts --encode",
    "decode": "ts-node src/demo.ts --decode",
    "test": "jest",
    "test:bench": "jest tests/bench.spec.ts",
    "test:real-sdk": "cross-env HOLOGRAM_USE_MOCK=false jest tests/real-sdk-benchmark.spec.ts",
    "test:real-sdk:watch": "cross-env HOLOGRAM_USE_MOCK=false jest tests/real-sdk-benchmark.spec.ts --watch",
    "test:watch": "jest --watch",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "bench:25g:optimized": "ts-node --transpile-only src/optimized-25g-benchmark.ts",
    "bench:25g:ultra": "ts-node --transpile-only src/ultra-high-performance-optimizer.ts",
    "bench:25g:run": "node run-optimized-25g-benchmark.js",
    "optimize:25g": "npm run build && npm run bench:25g:run"
  },
  "keywords": [
    "hologram",
    "virtual-infrastructure",
    "lattice",
    "transport",
    "storage",
    "compute"
  ],
  "author": "Hologram Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@hologram/sdk": "file:../../libs/sdk/node/sdk",
    "axios": "^1.4.0",
    "crypto": "^1.0.1",
    "ws": "^8.13.0",
    "zod": "^3.21.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
